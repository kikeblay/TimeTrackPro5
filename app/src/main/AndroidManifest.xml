<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetrackpro4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/imagen_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/imagen_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimeTrackPro4">
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.TimeTrackPro4.NoActionBar" />
        <activity
            android:name=".CrearCuentaActivity"
            android:exported="false" />
        <activity
            android:name=".ui.slideshow.FicharEntradaActivity"
            android:exported="false" />
        <activity
            android:name=".ui.slideshow.FicharSalidaActivity"
            android:exported="false" />
    </application>

</manifest>

